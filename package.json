{
  "name": "@rhds/tokens",
  "version": "1.0.0-beta.3",
  "description": "Red Hat Design System Tokens",
  "scripts": {
    "build": "wireit",
    "start": "wireit",
    "lint": "wireit",
    "format": "wireit",
    "test": "wireit"
  },
  "type": "module",
  "main": "js/tokens.cjs",
  "module": "js/tokens.js",
  "exports": {
    ".": {
      "import": "./js/tokens.js",
      "require": "./js/tokens.cjs"
    },
    "./values.js": {
      "import": "./js/values.js",
      "require": "./js/values.cjs"
    }
  },
  "files": [
    "css",
    "editor",
    "plugins",
    "js",
    "json",
    "scss"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "wireit": {
    "build": {
      "command": "node build.js",
      "files": [
        "tokens/**/*",
        "docs/**/*",
        "build.js"
      ],
      "output": [
        "build/**",
        "css/**",
        "editor/**",
        "js/**",
        "json/**",
        "scss/**"
      ]
    },
    "start": {
      "command": "wds",
      "dependencies": [
        "build"
      ]
    },
    "lint": {
      "command": "eslint lib plugins test docs"
    },
    "format": {
      "command": "eslint --fix lib plugins test docs"
    },
    "test": {
      "command": "tape-es test/*.js | tap-difflet",
      "dependencies": [
        "build"
      ]
    }
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": [
          "css",
          "scss"
        ],
        "path": "./editor/vscode.json"
      }
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@patternfly/eslint-config-elements": "^0.0.2-next.4",
    "@sindresorhus/slugify": "^2.1.0",
    "@web/dev-server": "^0.1.31",
    "css-tree": "^2.2.1",
    "eslint": "^8.23.0",
    "highlight.js": "^11.6.0",
    "markdown-it": "^13.0.1",
    "nunjucks": "^3.2.3",
    "style-dictionary": "^3.7.0",
    "tap-difflet": "^0.7.2",
    "tape-es": "^1.2.17",
    "wireit": "^0.4.3",
    "yaml": "^2.1.0"
  },
  "dependencies": {
    "postcss-value-parser": "^4.2.0",
    "stylelint": "^14.11.0"
  }
}
